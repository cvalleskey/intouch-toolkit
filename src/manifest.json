{
  "compatibleVersion": "51",
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Left",
      "identifier": "intouch-toolkit.align.left",
      "shortcut": "ctrl command ←",
      "script": "./align.js",
      "handlers": { "run": "left" }
    },
    {
      "name": "Horizontally",
      "identifier": "intouch-toolkit.align.horizontally",
      "shortcut": "ctrl command /",
      "script": "./align.js",
      "handlers": { "run": "horizontally" }
    },
    {
      "name": "Right",
      "identifier": "intouch-toolkit.align.right",
      "shortcut": "ctrl command →",
      "script": "./align.js",
      "handlers": { "run": "right" }
    },
    {
      "name": "Top",
      "identifier": "intouch-toolkit.align.top",
      "shortcut": "ctrl command ↑",
      "script": "./align.js",
      "handlers": { "run": "top" }
    },
    {
      "name": "Vertically",
      "identifier": "intouch-toolkit.align.vertically",
      "shortcut": "ctrl command .",
      "script": "./align.js",
      "handlers": { "run": "vertically" }
    },
    {
      "name": "Bottom",
      "identifier": "intouch-toolkit.align.bottom",
      "shortcut": "ctrl command ↓",
      "script": "./align.js",
      "handlers": { "run": "bottom" }
    },
    {
      "name": "Superscript",
      "identifier": "intouch-toolkit.auto-superscript",
      "shortcut": "ctrl command =",
      "script": "./auto-superscript.js",
      "handlers": { "run": "autoSuperscript" }
    },
    {
      "name": "Subscript",
      "identifier": "intouch-toolkit.auto-subscript",
      "shortcut": "ctrl command -",
      "script": "./auto-superscript.js",
      "handlers": { "run": "autoSubscript" }
    },
    {
      "name": "Manage Text Styles...",
      "identifier": "intouch-toolkit.generate-type-scale",
      "script": "./generate-type-scale.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    },
    {
      "name": "Update Selected Text Styles",
      "identifier": "intouch-toolkit.update-formatted-text-style",
      "shortcut": "ctrl command s",
      "script": "./update-formatted-text-style.js",
      "handlers": {
        "run": "updateFormattedTextStyle"
      }
    },
    {
      "name": "Sync Libary Text Styles",
      "identifier": "intouch-toolkit.sync-library-text-styles",
      "shortcut": "ctrl command option s",
      "script": "./update-formatted-text-style.js",
      "handlers": {
        "run": "syncLibraryTextStyles"
      }
    },
    {
      "name": "Generate columns...",
      "identifier": "intouch-toolkit.generate-grid",
      "shortcut": "ctrl command g",
      "script": "./generate-grid.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown",
          "LayersResized.finish": "onLayersResized"
        }
      }
    },
    {
      "name": "One",
      "identifier": "intouch-toolkit.generate-grid.one",
      "shortcut": "control option command 1",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridOne" }
    },
    {
      "name": "Two",
      "identifier": "intouch-toolkit.generate-grid.two",
      "shortcut": "control option command 2",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridTwo" }
    },
    {
      "name": "Three",
      "identifier": "intouch-toolkit.generate-grid.three",
      "shortcut": "control option command 3",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridThree" }
    },
    {
      "name": "Four",
      "identifier": "intouch-toolkit.generate-grid.four",
      "shortcut": "control option command 4",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridFour" }
    },
    {
      "name": "Six",
      "identifier": "intouch-toolkit.generate-grid.six",
      "shortcut": "control option command 6",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridSix" }
    },
    {
      "name": "Eight + Four",
      "identifier": "intouch-toolkit.generate-grid.eight-four",
      "shortcut": "control option command 8",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridEightFour" }
    },
    {
      "name": "Three + Nine",
      "identifier": "intouch-toolkit.generate-grid.three-nine",
      "shortcut": "control option command 9",
      "script": "./generate-grid.js",
      "handlers": { "run": "makeGridThreeNine" }
    },
    {
      "name": "Make padding...",
      "identifier": "intouch-toolkit.create-padding",
      "script": "./create-padding.js",
      "handlers": {
        "run": "onRun",
        "actions": { "Shutdown": "onShutdown" }
      }
    },
    {
      "name": "1 - XSmall (4px)",
      "identifier": "intouch-toolkit.create-padding.xsmall",
      "shortcut": "control command 1",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingXSmall"
      }
    },
    {
      "name": "2 - Small (8px)",
      "identifier": "intouch-toolkit.create-padding.small",
      "shortcut": "control command 2",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingSmall"
      }
    },
    {
      "name": "3 - Medium (16px)",
      "identifier": "intouch-toolkit.create-padding.medium",
      "shortcut": "control command 3",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingMedium"
      }
    },
    {
      "name": "4 - Large (32px)",
      "identifier": "intouch-toolkit.create-padding.large",
      "shortcut": "control command 4",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingLarge"
      }
    },
    {
      "name": "5 - XLarge (64px)",
      "identifier": "intouch-toolkit.create-padding.xlarge",
      "shortcut": "control command 5",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingXLarge"
      }
    },
    {
      "name": "6 - XXLarge (128px)",
      "identifier": "intouch-toolkit.create-padding.xxlarge",
      "shortcut": "control command 6",
      "script": "./create-padding.js",
      "handlers": {
        "run": "paddingXXLarge"
      }
    },
    {
      "name": "Export Styleguide",
      "identifier": "intouch-toolkit.styleguide.get-styleguide",
      "script": "./styleguide.js",
      "handlers": { "run": "exportStyleguide" }
    },
    {
      "name": "Spacing Units",
      "identifier": "intouch-toolkit.styleguide.generate-spacing",
      "script": "./styleguide.js",
      "handlers": { "run": "generateSpacing" }
    },
    {
      "name": "Color Range",
      "identifier": "intouch-toolkit.styleguide.generate-colors",
      "script": "./styleguide.js",
      "handlers": { "run": "generateColors" }
    },
    {
      "name": "Create Typography Scale",
      "identifier": "intouch-toolkit.create-typography-scale",
      "script": "./typography.js",
      "handlers": {
        "run": "updateFontScales"
      }
    },
    {
      "name": "Export to PDF...",
      "identifier": "intouch-toolkit.export-jpgs-to-pdf",
      "shortcut": "command option shift e",
      "script": "./export-jpgs-to-pdf.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      },
      "description": "Quick export all selected Artboards into a PDF document."
    },
    {
      "name": "Find and Replace Text",
      "identifier": "intouch-toolkit.find-and-replace-text",
      "shortcut": "ctrl command shift f",
      "script": "./find-and-replace-text.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    }
  ],
  "menu": {
    "title": "Intouch Toolkit",
    "items": [
      "intouch-toolkit.auto-superscript",
      "intouch-toolkit.auto-subscript",
      "-",
      "intouch-toolkit.find-and-replace-text",
      "-",
      {
